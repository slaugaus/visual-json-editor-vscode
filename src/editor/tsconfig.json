{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "jsx": "react",
        "jsxFactory": "createElement",
        "jsxFragmentFactory": "Fragment",
        "lib": [
            "ESNext", "DOM", "DOM.Iterable"
        ],
        "outDir": "../../media",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "moduleResolution": "bundler"
    }
}